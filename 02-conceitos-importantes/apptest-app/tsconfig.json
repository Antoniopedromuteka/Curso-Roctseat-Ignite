 
{
  "compilerOptions": {
 

    "target": "es2016",                                 
     "lib": ["dom","dom.Iterable","esnext"],            
     "jsx": "react-jsx",                             
      
    "module": "commonjs",                                
    "moduleResolution": "node",  
    "resolveJsonModule": true,
    "isolatedModules": true,                     
                        
     "noEmit": true,                                 
        
    "esModuleInterop": true,                           
   
    "forceConsistentCasingInFileNames": true,            
     
    "strict": true,                                     
 
    "skipLibCheck": true                                 
  },

  "include": ["src"]
}